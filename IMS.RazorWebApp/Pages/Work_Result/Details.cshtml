@page
@using IMS.Common
@model IMS.RazorWebApp.Pages.Work_Result.DetailsModel

@{
    ViewData["Title"] = "Details";
}

<h1>Working Result Details</h1>

<div>
    <h4>WorkingResult</h4>
    <hr />
    @if (Model.Message == Const.SUCCESS_READ_MSG)
    {
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.WorkingResult.CreatedBy)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.WorkingResult.CreatedBy)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.WorkingResult.Rating)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.WorkingResult.Rating)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.WorkingResult.Note)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.WorkingResult.Note)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.WorkingResult.Intern)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.WorkingResult.InternId)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.WorkingResult.Mentor)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.WorkingResult.MentorId)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.WorkingResult.Task)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.WorkingResult.TaskId)
        </dd>
    </dl>
    }
    else
    {
        <p>@Model.Message</p>
    }
</div>

<div>
    <a asp-page="./Edit" asp-route-id="@Model.WorkingResult.ResultId">Edit</a> |
    <a asp-page="./Index">Back to List</a>
</div>
